<% if users.exists? %>
  <% users.each do |user| %>
  <div class="col-md-8">
  <table>
  <table class="table">
    <thead>
      <tr>
      <td>image</td>
      <td>name</td>
      <td></td>
      </tr>
    </thead>
     <tbody>
        <tr>
          <td><%= attachment_image_tag user, :profile_image, size:'40x40', format: 'jpeg', fallback: "no_image.jpg" %></td>
          <td><%= user.name %></td>
          <td>フォロー数: <%= user.followings.count %></td>
          <td>フォロワー数: <%= user.followers.count %></td>
          <td><%= link_to  "Show", user_path(user) %></td>
        </tr>
      </tbody>
    </table>
  <% end %>
<% else %>
  <p>ユーザーはいません</p>
<% end %>